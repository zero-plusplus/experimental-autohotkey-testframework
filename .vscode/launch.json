{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "AutoHotkey Selected Debug",
      "type": "autohotkey",
      "request": "launch",
      "program": "${file}",
      "args": [],
      "useAutoJumpToError": true,
      "useDebugDirective": true,
      "usePerfTips": true,
    },
    {
      "name": "AutoHotkey Debug:UnitTest/AllTest.ahk2",
      "type": "autohotkey",
      "request": "launch",
      "program": "${workspaceFolder}/test/UnitTest/AllTest.ahk2",
      "args": [
        // "--no-auto-run"
      ],
      "useAutoJumpToError": true,
      "useDebugDirective": true,
      "usePerfTips": true,
    },
    {
      "name": "Gulp Debug:build",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
      "args": [
          "build"
      ],
      "sourceMaps": true,
      "resolveSourceMapLocations": [
          "${workspaceFolder}/**",
          "!**/node_modules/**"
      ],
      "skipFiles": [
          "<node_internals>/**"
      ]
  },
  ]
}